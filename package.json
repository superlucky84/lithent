{
  "name": "wwact",
  "version": "0.21.0",
  "description": "wwact",
  "private": true,
  "packageManager": "pnpm@7.6.0",
  "main": "./dist/wwact.umd.js",
  "module": "./dist/wwact.mjs",
  "scripts": {
    "dev:core": "vite",
    "dev:examples": "pnpm --filter wwact-examples dev",
    "build:core": "vite build",
    "build:helper": "pnpm --filter wwact-helper build",
    "watch:core": "vite build --watch",
    "watch:helper": "pnpm --filter wwact-helper watch",
    "build": "pnpm -r build",
    "dev": "pnpm watch:core & pnpm watch:helper & pnpm dev:examples",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "pnpm -r exec rm -rf dist",
    "clean:module": "pnpm -r exec rm -rf dist node_modules && rm -rf node_modules",
    "dev:docs": "pnpm --filter @wwact/docs dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/wwact.mjs",
      "require": "./dist/wwact.umd.js"
    },
    "./helper": {
      "types": "./helper/dist/index.d.ts",
      "import": "./helper/dist/wwactHelper.mjs",
      "require": "./helper/dist/wwactHelper.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=20.3.0"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https@github.com:superlucky84/wwact.git"
  },
  "homepage": "https://github.com/superlucky84/wwact",
  "license": "MIT",
  "keywords": ["wwact"],
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@nabla/vite-plugin-eslint": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "terser": "^5.17.7",
    "typescript": "^4.7.4",
    "vite": "3.2.7",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "volta": {
    "node": "20.3.0"
  }
}
