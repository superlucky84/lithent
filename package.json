{
  "type": "module",
  "name": "lithent",
  "version": "2.0.0",
  "description": "An extensible virtual DOM library for lightweight use in a variety of environments.",
  "packageManager": "pnpm@9.12.3",
  "main": "./dist/lithent.umd.js",
  "module": "./dist/lithent.mjs",
  "scripts": {
    "dev:core": "vite",
    "dev": "vite",
    "dev:examples": "pnpm watch:core & pnpm watch:helper & pnpm watch:devHelper & pnpm --filter lithent-examples dev",
    "dev:helper": "pnpm watch:core & pnpm --filter lithent-helper dev",
    "dev:devHelper": "pnpm watch:core & pnpm --filter lithent-dev-helper dev",
    "dev:ssr": "pnpm watch:core & pnpm --filter lithent-ssr dev",
    "dev:docs": "pnpm watch:core & pnpm watch:helper & pnpm --filter lithent-docs dev",
    "build:core": "vite build",
    "build:helper": "pnpm --filter lithent-helper build",
    "build:devHelper": "pnpm --filter lithent-dev-helper build",
    "build:hmrParser": "pnpm --filter @lithent/hmr-parser build",
    "build:lithentTemplateParser": "pnpm --filter @lithent/lithent-template-parser build",
    "build:lithentTemplateVite": "pnpm --filter @lithent/lithent-template-vite build",
    "build:lithentVite": "pnpm --filter @lithent/lithent-vite build",
    "build:lithentMdx": "pnpm --filter @lithent/lithent-mdx build",
    "build:jsxruntime": "pnpm --filter jsx-runtime build",
    "build:ssr": "pnpm --filter lithent-ssr build",
    "build:tag": "pnpm --filter lithent-tag build",
    "build:ftags": "pnpm --filter lithent-ftags build",
    "build:examples": "pnpm --filter lithent-examples build",
    "build:docs": "pnpm --filter lithent-docs build",
    "build:sequence": "pnpm build:core && pnpm build:jsxruntime && pnpm build:helper && pnpm build:devHelper && pnpm build:hmrParser && pnpm build:lithentTemplateParser && pnpm build:lithentTemplateVite && pnpm build:lithentMdx && pnpm build:lithentVite && pnpm build:ssr",
    "build:parallel": "pnpm --filter '!jsx-runtime' --filter '!lithent-helper' --filter '!lithent-dev-helper' --filter '!@lithent/hmr-parser' --filter '!@lithent/lithent-template-parser' --filter '!@lithent/lithent-template-vite' --filter '!@lithent/lithent-mdx' --filter '!@lithent/lithent-vite' --filter '!lithent-ssr' --filter '!create-lithent' build",
    "build": "pnpm run build:sequence && pnpm run build:parallel",
    "watch:core": "vite build --watch",
    "watch:helper": "pnpm --filter lithent-helper watch",
    "watch:devHelper": "pnpm --filter lithent-dev-helper watch",
    "test:core": "vitest run --reporter verbose",
    "test": "pnpm test:core && pnpm --filter lithent-helper test && pnpm --filter lithent-dev-helper test && pnpm --filter @lithent/hmr-parser test && pnpm --filter @lithent/lithent-mdx test && pnpm --filter @lithent/lithent-vite test && pnpm --filter lithent-ftags test",
    "test:lithentMdx": "pnpm --filter @lithent/lithent-mdx test",
    "test:lithentVite": "pnpm --filter @lithent/lithent-vite test",
    "test:dev": "vitest",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "pnpm -r exec rm -rf dist && rm -rf dist",
    "clean:module": "pnpm -r exec rm -rf dist node_modules && rm -rf node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/lithent.mjs",
      "require": "./dist/lithent.umd.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime/dist/index.d.ts",
      "import": "./jsx-runtime/dist/jsxRuntime.mjs",
      "require": "./jsx-runtime/dist/jsxRuntime.umd.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-runtime/dist/index.d.ts",
      "import": "./jsx-runtime/dist/jsxRuntime.mjs",
      "require": "./jsx-runtime/dist/jsxRuntime.umd.js"
    },
    "./helper": {
      "types": "./helper/dist/index.d.ts",
      "import": "./helper/dist/lithentHelper.mjs",
      "require": "./helper/dist/lithentHelper.umd.js"
    },
    "./devHelper": {
      "types": "./devHelper/dist/index.d.ts",
      "import": "./devHelper/dist/lithentDevHelper.mjs",
      "require": "./devHelper/dist/lithentDevHelper.umd.js"
    },
    "./ssr": {
      "types": "./ssr/dist/index.d.ts",
      "import": "./ssr/dist/lithentSsr.mjs",
      "require": "./ssr/dist/lithentSsr.umd.js"
    },
    "./tag": {
      "types": "./tag/dist/index.d.ts",
      "import": "./tag/dist/lithentTag.mjs",
      "require": "./tag/dist/lithentTag.umd.js"
    },
    "./ftags": {
      "types": "./ftags/dist/index.d.ts",
      "import": "./ftags/dist/lithentFTags.mjs",
      "require": "./ftags/dist/lithentFTags.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "jsx-runtime/dist",
    "jsx-runtime/src",
    "jsx-runtime/package.json",
    "helper/dist",
    "helper/src",
    "helper/package.json",
    "devHelper/dist",
    "devHelper/src",
    "devHelper/package.json",
    "ssr/dist",
    "ssr/src",
    "ssr/package.json",
    "tag/dist",
    "tag/src",
    "tag/package.json",
    "ftags/dist",
    "ftags/src",
    "ftags/package.json",
    "examples/dist",
    "examples/src",
    "examples/package.json"
  ],
  "author": "superlucky84",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/lithent.git"
  },
  "homepage": "https://github.com/superlucky84/lithent",
  "license": "MIT",
  "keywords": [
    "lithent"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "terser": "^5.18.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^2.1.2"
  },
  "volta": {
    "node": "20.3.0",
    "pnpm": "9.12.3"
  }
}
