{
  "presets": [
    [
      "@babel/preset-env", {
        "corejs":3,
        "useBuiltIns": "usage",
        "targets": "> 1%, last 2 versions"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ]
  ]
}
