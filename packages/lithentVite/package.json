{
  "type": "module",
  "name": "@lithent/lithent-vite",
  "version": "0.1.0",
  "description": "Vite plugin that wires Lithent-specific HMR boundaries and utilities.",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "dev:plugin": "vite --config vite.config.plugin.ts",
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "vitest run --reporter verbose",
    "test:dev": "vitest"
  },
  "devDependencies": {
    "lithent": "workspace:*",
    "lithent-dev-helper": "workspace:*"
  },
  "keywords": [
    "lithent",
    "vite",
    "hmr"
  ],
  "author": "superlucky84",
  "license": "MIT",
  "homepage": "https://github.com/superlucky84/lithent",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/lithent.git"
  },
  "dependencies": {
    "@babel/parser": "^7.26.2",
    "@babel/types": "^7.26.0",
    "magic-string": "^0.30.11"
  },
  "peerDependencies": {
    "lithent": "workspace:*",
    "vite": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "pnpm@9.12.3",
  "volta": {
    "node": "20.3.0"
  }
}
