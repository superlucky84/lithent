{
  "type": "module",
  "name": "@lithent/lithent-mdx",
  "version": "0.2.3",
  "description": "Vite plugin for MDX support in Lithent applications",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "vitest run --reporter verbose",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@mdx-js/rollup": "^3.1.0"
  },
  "devDependencies": {
    "@lithent/hmr-parser": "workspace:*",
    "lithent": "workspace:*",
    "vite": "^5.4.8"
  },
  "keywords": [
    "lithent",
    "vite",
    "mdx"
  ],
  "author": "superlucky84",
  "license": "MIT",
  "homepage": "https://github.com/superlucky84/lithent",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/lithent.git"
  },
  "peerDependencies": {
    "lithent": "^1.20.1",
    "vite": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "pnpm@9.12.3",
  "volta": {
    "node": "20.3.0"
  }
}
